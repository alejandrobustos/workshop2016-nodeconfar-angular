{
  "extends": "eslint:recommended",
  "plugins": [
    "angular"
  ],
  "env": {
    "es6": true,
    "browser": true,
    "jest": true
  },
  "ecmaFeatures": {
    "modules": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2017
  },
  "globals": {
    "angular": true,
    "module": true,
    "inject": true
  },
  "settings": {
    "angular": 2
  }
}
